local ClientId = game:GetService("RbxAnalyticsService"):GetClientId()
local Hwid = {
    [1] = "d1c3d91e-17a4-4a8e-9045-ba6eb0e1fcf8",
    ["d1c3d91e-17a4-4a8e-9045-ba6eb0e1fcf8"] = 1,
    [2] = "87704e1e-e5f4-4bc0-8727-99ad1f912194",
    ["87704e1e-e5f4-4bc0-8727-99ad1f912194"] = ,
}
local Key = {
    [1] = "MOON-RXSVS-ZPZUX",
    [2] = "MOON-ZTZUU-YRUWZ",
}
local KeyNumber = Hwid[ClientId]
if Hwid[KeyNumber] == ClientId then
if Key[KeyNumber] == _G.Key then
game.StarterGui:SetCore("SendNotification", {
       Title = "Moon Hub Premium script", 
       Text = ("Succeed Loading Gui !")
       })
wait(1.5)


_G.C = Color3.fromRGB(255,0,0)
if game:GetService("CoreGui"):FindFirstChild("DinoUI") then
	game:GetService("CoreGui"):FindFirstChild("DinoUI"):Destroy()
end



        
if game.CoreGui:FindFirstChild("SOMEXHUBMODILE") then
    game.CoreGui:FindFirstChild("SOMEXHUBMODILE"):Destroy()
end




spawn(function()
while wait() do
	pcall(function()
game.CoreGui.RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
 if child.Name == "ErrorPrompt" then 
  game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, game:GetService("Players").LocalPlayer)    
 end
end)
end)
end
end)



local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

        

